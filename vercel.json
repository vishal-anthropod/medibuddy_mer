{
  "version": 2,
  "functions": {
    "api/index.py": {
      "runtime": "vercel-python@3.11",
      "maxDuration": 60
    }
  },
  "rewrites": [
    { "source": "/static/(.*)", "destination": "/static/$1" },
    { "source": "/favicon.ico", "destination": "/static/favicon.ico" },
    { "source": "/(.*)", "destination": "/api/index.py" }
  ]
}


